<view class="rating-stars">
  <view
    wx:for="{{light}}"
    wx:key="{{this}}">
    <image mode="aspectFit"

           class="light"
           src="data:image/png;base64,"></image>

  </view>
  <view wx:if="{{isHalf}}">
    <image mode="aspectFit"
           class="half"
           src="data:image/png;base64,"
    ></image>
  </view>

  <view
    wx:for="{{gray}}"
    wx:key="{{this}}">
    <image mode="aspectFit"

           class="gray"
           src="data:image/png;base64,"></image>
  </view>
</view>